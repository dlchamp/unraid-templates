<?xml version="1.0"?>
<Container version="2">
  <Name>Unmonitorr</Name>
  <Repository>ghcr.io/dlchamp/unmonitorr:latest</Repository>
  <Registry>https://github.com/dlchamp/unmonitorr</Registry>
  <Network>bridge</Network>
  <MyIP/>
  <Shell>sh</Shell>
  <Privileged>false</Privileged>
  <Support>https://github.com/dlchamp/unmonitorr/issues</Support>
  <Project/>
  <Overview>Automatically unmonitor or remove items from Radarr and Sonarr.</Overview>
  <Category>MediaServer:Tools</Category>
  <WebUI>http://[IP]:[PORT:8080]/setup</WebUI>
  <TemplateURL>https://raw.githubusercontent.com/dlchamp/unraid-templates/main/unmonitorr.xml</TemplateURL>
  <Icon/>
  <ExtraParams/>
  <PostArgs/>
  <CPUset/>
  <DateInstalled/>
  <DonateText/>
  <DonateLink/>
  <Requires/>
  <Config Name="Log Level" Target="LOG_LEVEL" Default="info" Mode="" Description="Set the logging level" Type="Variable" Display="always" Required="false" Mask="false">info</Config>
  <Config Name="Webhook Port" Target="9080" Default="9080" Mode="tcp" Description="Set the port for webhook communication" Type="Port" Display="always" Required="true" Mask="false">8080</Config>
  <Config Name="Config" Target="/app/unmonitorr/data/" Default="" Mode="rw" Description="Persistent config and logs" Type="Path" Display="always" Required="true" Mask="false">/mnt/user/appdata/unmonitorr/</Config>
</Container>
